      "validation": "^[\\s\\S]*$"
    },
    {
      "title": "Price",
      "field": "price",
      "type": "number",
      "required": true,
      "placeholder": "Enter price",
      "min": 0,
      "validation": "^\\d+(\\.\\d{1,2})?$"
